<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<TitledPane alignment="CENTER" animated="false" collapsible="false" prefWidth="500.0" text="Team info" xmlns="http://javafx.com/javafx/8.0.92" xmlns:fx="http://javafx.com/fxml/1">
   <content>
      <AnchorPane styleClass="sub_background">
         <children>
             <GridPane AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="10.0">
                 <columnConstraints>
                     <ColumnConstraints hgrow="NEVER" prefWidth="125.0" />
                     <ColumnConstraints hgrow="SOMETIMES" maxWidth="300.0" prefWidth="200.0" />
                 </columnConstraints>
                 <rowConstraints>
                     <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                     <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                     <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                     <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES" />
                 </rowConstraints>
                 <children>
                     <Label text="Team name" />
                     <Label fx:id="teamName" text="Placeholder" GridPane.columnIndex="1" />
                     <Label text="Team leader" GridPane.rowIndex="1" />
                     <Label fx:id="teamLeader" text="Placeholder" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                     <Label text="Team leader email" GridPane.rowIndex="2" />
                     <Label fx:id="teamLeadEmail" text="Placeholder" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                     <Label text="Team members" GridPane.rowIndex="3" />
                     <Label fx:id="teamMembers" text="Placeholder" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                 </children>
             </GridPane>
            <ImageView fitHeight="200.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" AnchorPane.rightAnchor="10.0" AnchorPane.bottomAnchor="10.0">
               <image>
                  <Image url="@LogoUTimproved.png" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
   </content>
</TitledPane>
